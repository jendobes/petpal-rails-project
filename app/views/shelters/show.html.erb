<h3> <%= @shelter.name %> </h3>

<div id="petIndex">
</div>

<%= link_to "See pets at this shelter", shelter_pets_path(@shelter), :id => "showPets" %>

<div id="profiles"></div>

<script id="pet-template" type="text/x-handlebars-template">
  <div class="row">
    <div class="col s12 m7">
      <div class="card">
        <div class="card-image">
            <span class="card-title"></span>
        </div>
        <div class="card-content">
          <ul id="attributes">
          <li><{{name}}</li>
          <li><{{breed}}</li>
          <li><{{gender}}</li>
          <li><{{age}}</li>
          <li><{{bio}}</li>
          </ul>
        </div>
      </div>
    </div>
  </div>
</script>
